{
    "name": "woub-chat/entity",
    "description": "Pattern for generating php entities.",
    "type": "script",
    "keywords": ["entity", "script"],
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.0",
        "woub-chat/text": "^0.0.4"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Bfg\\Entity\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bfg\\Entity\\ServiceProvider"
            ],
            "aliases": {
                "EntityCarrier": "Bfg\\Entity\\Facade"
            }
        }
    }
}
